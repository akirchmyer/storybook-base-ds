{
  "name": "storybook-base-ds",
  "version": "1.0.0",
  "description": "Base template for a package that creates design systems using storybook, React TypeScript primitives & composites, sass and npm",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "format": "prettier --write \"./src/**/*.+(js|ts|tsx|json)\"",
    "lint": "eslint ./src --ext .ts --ext .tsx",
    "lint:fix": "eslint ./src --ext .ts --ext .tsx --fix && npm run format",
    "test": "npm run lint && npm run unit",
    "unit": "jest",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "main": "dist/index.js",
  "author": "Andrew Kirchmyer",
  "license": "MIT",
  "keywords": [
    "storybook",
    "Design System",
    "sass",
    "grid",
    "primitives",
    "composites"
  ],
  "bugs": {
    "url": "https://github.com/akirchmyer/storybook-base-ds/issues"
  },
  "homepage": "https://github.com/akirchmyer/storybook-base-ds",
  "devDependencies": {
    "@babel/core": "^7.18.10",
    "@storybook/addon-actions": "^6.5.10",
    "@storybook/addon-essentials": "^6.5.10",
    "@storybook/addon-interactions": "^6.5.10",
    "@storybook/addon-links": "^6.5.10",
    "@storybook/builder-webpack4": "^6.5.10",
    "@storybook/manager-webpack4": "^6.5.10",
    "@storybook/react": "^6.5.10",
    "@storybook/testing-library": "^0.0.13",
    "@types/jest": "^28.1.6",
    "@types/node": "^18.7.3",
    "@types/react": "^18.0.17",
    "@typescript-eslint/eslint-plugin": "^5.39.0",
    "@typescript-eslint/parser": "^5.33.0",
    "babel-loader": "^8.2.5",
    "eslint": "^8.21.0",
    "eslint-config-prettier": "^8.5.0",
    "jest": "^28.1.3",
    "prettier": "^2.7.1",
    "ts-jest": "^28.0.8",
    "typescript": "^4.7.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/akirchmyer/storybook-base-ds.git"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
